{
  "name": "@aureooms/js-rational",
  "description": "Rational numbers for JavaScript",
  "version": "3.0.1",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-rational/issues"
  },
  "dependencies": {
    "@aureooms/js-itertools": "^4.0.0"
  },
  "devDependencies": {
    "@aureooms/js-integer": "9.0.0",
    "@aureooms/js-integer-big-endian": "10.1.0",
    "@aureooms/js-number": "3.1.0",
    "@aureooms/js-prime": "4.0.0",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "ava": "3.11.1",
    "bn.js": "5.1.2",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "int": "0.2.0",
    "np": "6.3.2",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/aureooms/js-rational",
  "keywords": [
    "algorithm",
    "arithmetic",
    "bigdec",
    "bigdecimal",
    "complexity",
    "exact",
    "fractions",
    "javascript",
    "js",
    "numbers",
    "rational"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-rational.git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
